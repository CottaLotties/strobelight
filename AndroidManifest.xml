<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.stwalkerster.android.apps.strobelight"
      android:versionCode="1"
      android:versionName="1.0" android:installLocation="preferExternal">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:allowClearUserData="false" android:debuggable="true">
        <activity android:name=".StrobeLightConfig"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>



<uses-permission android:name="android.permission.CAMERA"></uses-permission>

</manifest> 